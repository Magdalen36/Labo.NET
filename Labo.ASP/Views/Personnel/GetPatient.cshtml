@model Labo.ASP.Models.Forms.PatientForm

@{
    ViewData["Title"] = "GetPatient";
}


<h4>Gestion des injections</h4>
<h5>Veuillez indiquer le numéro d'injection</h5>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="GetPatient">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                @*<label asp-for="Id" class="control-label"></label>*@
                <input asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Accéder" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Retour à l'espace personnel</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
